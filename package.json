{
  "name": "todomagotchi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh -c 'export $(cat .env | grep BETTER_AUTH_URL | xargs) && next dev --turbopack -p ${BETTER_AUTH_URL##*:}'",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "seed": "prisma migrate reset",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:trace": "TEST_TRACE=true playwright test",
    "test:e2e:auth": "playwright test e2e/auth.spec.ts --workers=1",
    "test:e2e:auth:headed": "playwright test e2e/auth.spec.ts --headed --workers=1",
    "test:e2e:auth:trace": "TEST_TRACE=true playwright test e2e/auth.spec.ts --workers=1",
    "test:e2e:live": "playwright test e2e/live-data.spec.ts --workers=2",
    "test:e2e:live:headed": "playwright test e2e/live-data.spec.ts --headed --workers=2",
    "test:e2e:live:trace": "TEST_TRACE=true playwright test e2e/live-data.spec.ts --workers=2",
    "test:e2e:messaging": "playwright test e2e/messaging.spec.ts --workers=2",
    "test:e2e:messaging:headed": "playwright test e2e/messaging.spec.ts --headed --workers=2",
    "test:e2e:messaging:trace": "TEST_TRACE=true playwright test e2e/messaging.spec.ts --workers=2",
    "test:report": "tsx scripts/generate-test-report.ts",
    "test:report:cleanup": "tsx scripts/generate-test-report.ts --cleanup",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.5",
    "better-auth": "^1.3.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.546.0",
    "next": "15.5.4",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.13",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.13"
  }
}
